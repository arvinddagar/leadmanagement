<div class="container">
<h1>Categories Listing </h1>
<div style="float:right;"><%= link_to 'New Category', new_admin_category_path , :class=>"btn btn-primary"%></div>
<table class='table'>
  <thead>
    <tr>
      <th>Name</th>
    
    </tr>
  </thead>

  <tbody>
    <% @categories.each do |category| %>
      <tr>
        <td><%= category.name %></td>
        <td><%= link_to 'Show', admin_category_path(category) %> |
        <%= link_to 'Edit', edit_admin_category_path(category) %> |
        <%= link_to 'Destroy', admin_category_path(category), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>

  </tbody>
</table>
    <%#= paginate @daily_updates %>

<%#= link_to 'New Daily update', new_daily_update_path %>
</div>