<section id="main-content">
  <section class="wrapper">
    <h3><i class="fa fa-angle-right"></i>Reports</h3>
    <div class="row">
        <div class="col-md-12">
        </div>
      <div class="col-md-12 mt">
        <div class="content-panel">
        <div>
<h3>Reports of accounts expired</h3>
            
            <table class='table'>
            <thead>
              <tr>
                <th>Client name</th>
                <th colspan="2">Status</th>
                <th colspan="2">Domain Name</th>
                <th colspan="2">Renewal Date</th>               
              </tr>
            </thead>
            <tbody>
            <tr>
            <td>
            <h4>To be expired in upcoming 7 days </h4>
            <%@c.each do |contract|%>
            <tr>
              <td >
                <%=contract.daily_update.contact_person%></br>
                <%=contract.daily_update.number %>
              </td>
              <td colspan="2"><%=contract.status%></td>
              <td colspan="2"><%=contract.domain_name%></td>
              <td colspan="2"><%=format_date(contract.plans.last.renewal_date)%></td>
              </tr>
            <%end%>
            </td>
            </tr>
            <tr>
            <td>
            <h4>Plans  expired</h4>
            <%@contract.each do |contract|%>
            <tr>
               <td >
                <%=contract.daily_update.contact_person%></br>
                <%=contract.daily_update.number %>
              </td>
              <td colspan="2"><%=contract.status%></td>
              <td colspan="2"><%=contract.domain_name%></td>
              <td colspan="2"><%=format_date(contract.plans.last.renewal_date)%></td>
              </tr>
            <%end%>
            </td>
            </tr>
            </tbody>
            </table>
             </div>
        </div>
      </div>
    </div>
  </section>   
</section> 